{
  "name": "doof-backend",
  "version": "1.0.0",
  "description": "Backend for Doof food recommendation app",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --ext js,json --exec 'npx tsx server.js'",
    "test": "vitest run",
