{
  "name": "doof-backend",
  "version": "1.0.0",
  "description": "Backend for Doof food recommendation app",
  "main": "dist/server.js", 
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js", 
    "dev": "nodemon --ext ts,json --exec 'npx tsx server.ts'", 
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "@googlemaps/google-maps-services-js": "^3.3.42"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "nodemon": "^3.0.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
